<p id="notice"><%= notice %></p>

<p>
  <strong>Day:</strong>
  <%= @record.day %>
</p>

<p>
  <strong>Height:</strong>
  <%= @record.height %>
</p>

<p>
  <strong>Weight:</strong>
  <%= @record.weight %>
</p>

<p>
  <strong>Bmi:</strong>
  <%= @record.bmi %>
</p>

<p>
  <strong>Highblood:</strong>
  <%= @record.highblood %>
</p>

<p>
  <strong>Lowblood:</strong>
  <%= @record.lowblood %>
</p>

<p>
  <strong>Diary:</strong>
  <%= @record.diary %>
</p>

<p>
  <strong>Picture:</strong>
  <%= puts @record %>
    <% @record.picture_urls.each do |picture_url| %>
      <% next if picture_url.blank? %>
      <%= image_tag(picture_url, size: '160x120') %>
    <% end %>
</p>

<p>
  <strong>Targetbmi:</strong>
  <%= @record.targetbmi %>
</p>

<p>
  <strong>Targetweight:</strong>
  <%= @record.targetweight %>
</p>

<p>
  <strong>Compareweight:</strong>
  <%= @record.compareweight %>
</p>

<%= link_to 'Edit', edit_record_path(@record) %> |
<%= link_to 'Back', records_path %>
